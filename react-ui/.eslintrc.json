{
    "parser": "babel-eslint",
    "extends": ["airbnb", "prettier", "prettier/flowtype", "prettier/react"],
    "plugins": ["react", "jsx-a11y", "import", "flowtype", "prettier"],
    "env": {
      "jest": true,
      "browser": true
    },
    "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"]}]
    }
  }
  